.Container
  position fixed
  left 0
  bottom 0
  z-index 10
  width 100%
  background-color #fff
  border-top 1px solid #eee
  .nav
    display flex
    height 98px
    .navItem
      width 20%
      display flex
      flex-direction column
      align-items center
      justify-content center
      &.on
        color #b4282d
      .iconfont
        font-size 40px
      .text
        font-size 24px
        margin-top 10px